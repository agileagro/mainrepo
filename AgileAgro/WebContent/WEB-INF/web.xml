    <servlet>
    <description></description>
    <display-name>KbUpdate</display-name>
    <servlet-name>KbUpdate</servlet-name>
    <servlet-class>com.agile.servlets.KbUpdate</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>newinstance</display-name>
    <servlet-name>newinstance</servlet-name>
    <servlet-class>com.agile.servlets.newinstance</servlet-class>
  </servlet>
  <resource-ref>
    <res-ref-name>jdbc/DefaultDB</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
  </resource-ref>
</web-app>